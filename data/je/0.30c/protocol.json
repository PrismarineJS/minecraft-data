          {
            "name": "z",
            "type": "i16"
          },
          {
            "name": "yaw",
            "type": "u8"
          },
          {
            "name": "pitch",
            "type": "u8"
          }
        ]
      ],
      "packet_message": [
        "container",
        [
          {
            "name": "unused",
            "type": "u8"
          },
          {
            "name": "message",
            "type": "string"
          }
        ]
      ],
      "packet": [
        "container",
        [
          {
            "name": "name",
            "type": [
              "mapper",
              {
                "type": "u8",
                "mappings": {
                  "0x00": "player_identification",
                  "0x05": "set_block",
                  "0x08": "position",
                  "0x0d": "message"
                }
              }
            ]
          },
          {
            "name": "params",
            "type": [
              "switch",
              {
                "compareTo": "name",
                "fields": {
                  "player_identification": "packet_player_identification",
                  "set_block": "packet_set_block",
                  "position": "packet_position",
                  "message": "packet_message"
                }
              }
            ]
          }
        ]
      ]
    }
  },
