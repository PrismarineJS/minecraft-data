          {
            "name": "protocol_version",
            "type": "u8"
          },
          {
            "name": "username",
            "type": "string"
          },
          {
            "name": "verification_key",
            "type": "string"
          },
          {
            "name": "unused",
            "type": "i8"
          }
        ]
      ],
      "packet_set_block": [
        "container",
        [
          {
            "name": "x",
            "type": "i16"
          },
          {
            "name": "y",
            "type": "i16"
