      "cstring",
      {
        "encoding": "utf-16"
      }
    ],
    "slot": [
      "container",
      [
        {
          "name": "blockId",
          "type": "i16"
        },
        {
          "anon": true,
          "type": [
            "switch",
            {
              "compareTo": "blockId",
              "fields": {
                "-1": "void"
              },
              "default": [
                "container",
                [
                  {
                    "name": "itemCount",
                    "type": "i8"
                  },
                  {
                    "name": "itemDamage",
                    "type": "i16"
                  },
                  {
                    "name": "nbtData",
                    "type": "compressedNbt"
                  }
                ]
              ]
            }
          ]
        }
      ]
    ],
    "entityMetadataItem": [
      "switch",
      {
        "compareTo": "$compareTo",
        "fields": {
          "0": "i8",
          "1": "i16",
          "2": "i32",
          "3": "f32",
          "4": "string",
          "5": "slot",
          "6": [
            "container",
            [
              {
                "name": "x",
                "type": "i32"
              },
              {
                "name": "y",
                "type": "i32"
              },
              {
                "name": "z",
                "type": "i32"
              }
            ]
          ]
        }
      }
    ],
    "entityMetadata": [
      "entityMetadataLoop",
      {
        "endVal": 127,
        "type": [
          "container",
          [
            {
              "anon": true,
              "type": [
                "bitfield",
                [
                  {
                    "name": "type",
                    "size": 3,
                    "signed": false
                  },
                  {
                    "name": "key",
                    "size": 5,
                    "signed": false
                  }
                ]
              ]
            },
            {
              "name": "value",
              "type": [
                "entityMetadataItem",
                {
                  "compareTo": "type"
                }
              ]
            }
          ]
        ]
      }
    ]
  },
