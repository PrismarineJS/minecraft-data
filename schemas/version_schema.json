{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "version",
  "type": "object",
  "uniqueItems": true,
  "properties" : {
    "version":
    {
      "type":"integer"
    },
    "minecraftVersion":{
      "type":"string",
      "pattern": "([0-9]+\\.[0-9]+(\\.[0-9]+)?[a-z]?(-pre[0-9]+)?)|([0-9]{2}w[0-9]{2}[a-z])"
    },
    "majorVersion":{
      "type":"string",
      "pattern": "[0-9]+\\.[0-9]+[a-z]?"
    }
  },
  "additionalProperties": false
}