{
  "name": "web-minecraft-data",
  "version": "1.0.0",
  "description": "Web interface to minecraft-data",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "standard",
    "build": "browserify index.js -o bundle.js",
    "start": "beefy index.js:bundle.js 9998",
    "gh-publish": "git checkout gh-pages && git merge web && npm run build && git add -f bundle.js && git commit -m 'publish' ; git push origin gh-pages && git checkout web"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PrismarineJS/minecraft-data/tree/web"
  },
  "keywords": [
    "web",
    "minecraft",
    "data"
  ],
  "author": "Romain Beaumont <romain.rom1@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "brfs": "^1.4.3",
    "docson": "^1.0.2",
    "json-stable-stringify": "^1.0.1",
    "marked": "^0.3.5",
    "minecraft-assets": "^1.1.2",
    "minecraft-data": "^2.37.5",
    "query-string": "^3.0.3"
  },
  "devDependencies": {
    "beefy": "^2.1.8",
    "browserify": "^13.0.0",
    "standard": "^12.0.1"
  }
}
